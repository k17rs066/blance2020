<Window x:Class="balance.Views.TagResult"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:balance.Views"
        mc:Ignorable="d"
        Title="鬼ごっこ結果表示" Height="450" Width="800" WindowStartupLocation="CenterScreen">
    <Grid>
        <Label x:Name="Result"  Margin="113,65,102,269" FontSize="60" VerticalAlignment="Center" HorizontalAlignment="Center" FontWeight="Bold"/>

        <Label Content="経過時間："  x:Name="TimeResult" Margin="190,175,367,180"  FontSize="30"/>

        <Label Content=""  x:Name="TimeResult2" Margin="430,175,127,180"  FontSize="30" VerticalAlignment="Center" HorizontalAlignment="Center"/>
        <Label Content="鬼の速さ：" x:Name="TagSpeed" Margin="190,244,367,125" FontSize="30"/>
        <Label Content="" x:Name="TagSpeed2" Margin="542,244,240,125" FontSize="30" VerticalAlignment="Center" HorizontalAlignment="Center"/>



        <Button x:Name="Back" Click="Back_Click" Content="終了する" Margin="113,325,467,34"  FontSize="40"/>

        <Button x:Name="RePlayGame" Click="RePlayGame_Click" Content="もう一度" Margin="418,325,162,34"  FontSize="40"/>

    </Grid>
</Window>
