        <Label Content="スコア" HorizontalAlignment="Left" Margin="278,172,0,0" VerticalAlignment="Top" Height="98" Width="179" FontSize="68" RenderTransformOrigin="0.481,0.5" FontWeight="Bold"/>

        <Label x:Name="ftgamemode" Content="" HorizontalAlignment="Left" Margin="268,32,0,0" VerticalAlignment="Top" Height="91" Width="410" FontSize="67" HorizontalContentAlignment="Center" FontWeight="Bold"/>
        <Label x:Name="ftresult" Content="" HorizontalAlignment="Left" Margin="548,172,0,0" VerticalAlignment="Top" Height="97" Width="338" FontSize="68" FontWeight="Bold"/>
        <Label x:Name="fttokikai" Content="" HorizontalAlignment="Left" Margin="278,305,0,0" VerticalAlignment="Top" Height="77" Width="216" FontSize="46" />
        <Label x:Name="ftresulttime" Content="" HorizontalAlignment="Left" Margin="548,305,0,0" VerticalAlignment="Top" Height="85" Width="352" FontSize="46" />

    </Grid>
</Window>
